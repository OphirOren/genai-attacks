{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "additionalProperties": false,
  "properties": {
    "description": {
      "type": "string"
    },
    "frameworks": {
      "items": {
        "type": "string"
      },
      "type": "array"
    },
    "id": {
      "type": "string"
    },
    "references": {
      "items": {
        "additionalProperties": false,
        "properties": {
          "author": {
            "type": "string"
          },
          "href": {
            "format": "uri",
            "type": "string"
          },
          "title": {
            "type": "string"
          }
        },
        "required": [
          "author",
          "href",
          "title"
        ],
        "type": "object"
      },
      "type": "array"
    },
    "summary": {
      "type": "string"
    },
    "tactics": {
      "items": {
        "type": "string"
      },
      "type": "array"
    },
    "title": {
      "type": "string"
    }
  },
  "required": [
    "id",
    "references",
    "summary",
    "tactics",
    "title"
  ],
  "type": "object"
}
